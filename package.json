{
  "name": "electron-path-ipc",
  "version": "0.0.1",
  "description": "Express style path for Electron IPC",
  "author": "StudiMax Ent.",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "compile": "tsc",
    "test": "yarn run coverage",
    "coverage": "nyc mocha",
    "gts:check": "gts check ./src/**/*.{js,jsx,ts,tsx,vue}",
    "gts:clean": "gts clean",
    "gts:fix": "gts fix ./src/**/*.{js,jsx,ts,tsx,vue}",
    "gts:lint": "gts lint ./src/**/*.{js,jsx,ts,tsx,vue}",
    "prepublishOnly": "yarn run compile"
  },
  "peerDependencies": {
    "electron": ">= 9.1.2"
  },
  "devDependencies": {
    "@types/chai": "^4.2.16",
    "@types/mocha": "^8.2.2",
    "@types/proxyquire": "^1.3.28",
    "@types/uuid": "^8.3.0",
    "chai": "^4.3.4",
    "electron": "^12.0.2",
    "electron-mock-ipc": "^0.3.9",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "gts": "^3.1.0",
    "mocha": "^8.3.2",
    "nyc": "^15.1.0",
    "proxyquire": "^2.1.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "path-to-regexp": "^6.2.0",
    "uuid": "^8.3.2"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "directories": {
    "test": "test"
  },
  "repository": "https://github.com/studimax/electron-path-ipc.git"
}
